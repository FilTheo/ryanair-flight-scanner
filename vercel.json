{
    "version": 2,
    "public": "static",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/(.*)",
            "destination": "/static/$1"
        }
    ],
    "functions": {
        "api/index.py": {
            "maxDuration": 60
        }
    }
}